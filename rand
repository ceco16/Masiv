#include <iostream>
#include<cstdlib>
#include<iomanip>
using namespace std;

int main()
{
    int a[3][4];
    for(int i=0;i<3;i++)
    {
    for(int j=0;j<4;j++)
    {
    a[i][j]=rand()%20+20;    
    }
    }
    for(int i=0;i<3;i++)
    {
    for(int j=0;j<4;j++)
    {
        cout<<setw(3);
        cout<<a[i][j]<<" ";
    }
    cout<<endl;
}
}
